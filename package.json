{
  "name": "elysia-schema-gen",
  "version": "0.0.1",
  "description": "CLI tool to automatically generate Elysia schemas from class definitions",
  "type": "module",
  "author": "Naviary Sanctuary",
  "license": "MIT",
  "keywords": [
    "elysia",
    "schema",
    "typebox",
    "codegne",
    "cli",
    "bun"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Naviary-Sanctuary/elysia-schema-gen.git"
  },
  "bin": {
    "elyisa-schema-gen": "./dist/cli.js"
  },
  "scripts": {
    "build": "bun build ./src/clit.ts --outdir ./dist --target node",
    "dev": "bun run ./src/cli.ts",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "glob": "^13.0.0",
    "ts-morph": "^27.0.2"
  },
  "devDependencies": {
    "@types/bun": "1.3.7",
    "@types/glob": "^9.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "oxfmt": "^0.27.0",
    "oxlint": "^1.42.0"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  }
}
